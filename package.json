{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "repository": "git@github.com:StephenGrider/ReduxSimpleStarter.git",
  "scripts": {
    "start": "gulp && node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "server": "./node_modules/.bin/nodemon ./server.js --exec ./node_modules/.bin/babel-node --presets es2015",
    "build" : "webpack -p"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.1.18",
    "body-parser": "^1.15.2",
    "bulma": "^0.2.3",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "font-awesome": "^4.7.0",
    "gulp": "^3.9.1",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "laravel-elixir": "^6.0.0-15",
    "lodash": "^3.10.1",
    "mocha": "^2.4.5",
    "mongodb": "^2.2.16",
    "nodemon": "^1.11.0",
    "re-base": "^2.5.1",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.7",
    "react-addons-update": "^15.4.1",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-form": "^6.3.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
